"VertexlitGeneric"
{
	"$baseTexture" "models/weapons/c_models/c_cratesmasher/c_cratesmasher"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6

	"$yellow" "0"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "5"
	"$phongfresnelranges" "[.25 .5 1]"
	"$basemapalphaphongmask" "1"
	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "2"

	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}

		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}

		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}

		"YellowLevel"
		{
			"resultVar" "$yellow"
		}

		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}
